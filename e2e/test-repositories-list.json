{
  "nestjs": {
    "name": "NestJS Cats App",
    "type": "clone",
    "repo": "https://github.com/nestjs/nest",
    "path": "sample/01-cats-app",
    "branch": "master",
    "assertions": {
      "shouldDetectFramework": "nestjs",
      "shouldHaveEndpoints": true,
      "minEndpoints": 3
    }
  },
  "express": {
    "name": "Express Prisma Example",
    "notes":"used to test gitDiff functionality",
    "type": "clone",
    "repo": "https://github.com/prisma/prisma-examples",
    "path": "orm/express",
    "branch": "latest",
    "modifications": {
      "package.json": {
        "gitDiff-test": "added"
      }
    },
    "assertions": {
      "shouldDetectFramework": "express",
      "shouldHaveEndpoints": true,
      "minEndpoints": 5
    }
  },
