{
  "nestjs": {
    "name": "NestJS Cats App",
    "type": "clone",
    "repo": "https://github.com/nestjs/nest",
    "path": "sample/01-cats-app",
    "branch": "master",
    "assertions": {
      "shouldDetectFramework": "NestJS",
      "shouldHaveEndpoints": true,
      "minEndpoints": 3
    }
  },
  "express": {
    "name": "Express Multi-Router",
    "type": "clone",
    "notes": "Contains no package.json",
    "repo": "https://github.com/expressjs/express",
    "path": "examples/multi-router",
    "branch": "master",
    "assertions": {
      "shouldDetectFramework": "Express",
      "shouldHaveEndpoints": true,
      "minEndpoints": 2
    }
  },
  "fastify": {
    "name": "Fastify Demo",
    "type": "clone",
    "repo": "https://github.com/fastify/demo",
    "path": ".",
    "branch": "main",
    "assertions": {
      "shouldDetectFramework": "Fastify",
      "shouldHaveEndpoints": true,
      "minEndpoints": 5
    }
  },
  "express-generator": {
    "name": "Express Generator App",
    "type": "generate",
    "generate": {
      "command": "npx",
      "args": ["express-generator", "express-generator", "--no-git"]
    },
    "assertions": {
      "shouldDetectFramework": "Express",
      "shouldHaveEndpoints": true
    }
  }
}
