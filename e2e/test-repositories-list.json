{
  "nestjs": {
    "name": "NestJS Cats App",
    "type": "clone",
    "repo": "https://github.com/nestjs/nest",
    "path": "sample/01-cats-app",
    "branch": "master",
    "assertions": {
      "shouldDetectFramework": "NestJS",
      "shouldHaveEndpoints": true,
      "minEndpoints": 3
    }
  },
  "express": {
    "name": "Express Prisma Example",
    "type": "clone",
    "repo": "https://github.com/prisma/prisma-examples",
    "path": "orm/express",
    "branch": "latest",
    "assertions": {
      "shouldDetectFramework": "Express",
      "shouldHaveEndpoints": true,
      "minEndpoints": 5 
    }
  },
  "fastify": {
    "name": "Fastify Demo",
    "type": "clone",
    "repo": "https://github.com/fastify/demo",
    "path": ".",
    "branch": "main",
    "assertions": {
      "shouldDetectFramework": "Fastify",
      "shouldHaveEndpoints": true,
      "minEndpoints": 5
    }
  },
  "express-2": {
    "name": "Express Generator App",
    "type": "generate",
    "generate": {
      "command": "npx",
      "args": ["express-generator", "express-2", "--no-git"]
    },
    "assertions": {
      "shouldDetectFramework": "Express",
      "shouldHaveEndpoints": true,
      "minEndpoints": 2 
    }
  },
  "react": {
    "name": "React Onborda",
    "type": "clone",
    "repo": "https://github.com/uixmat/onborda",
    "path": ".",
    "branch": "main",
    "assertions": {}
  },
  "react-2": {
    "name": "React Router Generated",
    "type": "generate",
    "generate": {
      "command": "npx",
      "args": ["create-react-router@latest", "react-2", "-y"]
    },
    "assertions": {
      "shouldDetectFramework": "React"
    }
  },
  "vue": {
    "name": "Vue Pinia",
    "type": "clone",
    "repo": "https://github.com/vuejs/pinia",
    "path": ".",
    "branch": "v2",
    "assertions": {
      "shouldDetectFramework": "Vue",
      "shouldHaveEndpoints": true,
      "minEndpoints": 1 
    }
  },
  "vue-2": {
    "name": "Vue Generated",
    "type": "generate",
    "generate": {
      "command": "npx",
      "args": ["create-vue@latest", "vue-2", "--typescript", "--jsx", "--router", "--pinia", "--vitest", "--eslint", "--prettier"]
    },
    "assertions": {
      "shouldDetectFramework": "Vue"
    }
  },
  "angular": {
    "name": "Angular Walk My Dog",
    "type": "clone",
    "repo": "https://github.com/angular/examples",
    "path": "walk-my-dog",
    "branch": "main",
    "assertions": {
      "shouldDetectFramework": "Angular"
    }
  },
  "angular-2": {
    "name": "Angular Generated",
    "type": "generate",
    "generate": {
      "command": "npx",
      "args": ["@angular/cli@latest", "new", "angular-2", "--standalone", "--routing", "--style=scss", "--strict", "--skip-git"]
    },
    "assertions": {
      "shouldDetectFramework": "Angular"
    }
  },
  "svelte": {
    "name": "Svelte TodoMVC",
    "type": "clone",
    "repo": "https://github.com/sveltejs/examples",
    "path": "examples/todomvc",
    "branch": "main",
    "assertions": {
      "shouldDetectFramework": "Svelte"
    }
  },
  "vanilla": {
    "name": "Vanilla JS NASA APOD",
    "type": "clone",
    "repo": "https://github.com/ymw0331/vanilla-javascript-20-projects",
    "path": "014-nasa-apod",
    "branch": "master",
    "assertions": {
      "shouldDetectFramework": "unknown"
    }
  },
  "nextjs": {
    "name": "Next.js Hello World",
    "type": "clone",
    "repo": "https://github.com/vercel/next.js",
    "path": "examples/hello-world",
    "branch": "canary",
    "assertions": {
      "shouldDetectFramework": "Next.js",
      "shouldHaveEndpoints": true,
      "minEndpoints": 1
    }
  },
  "nextjs-2": {
    "name": "Next.js Generated",
    "type": "generate",
    "generate": {
      "command": "npx",
      "args": ["create-next-app@latest", "nextjs-2", "--app", "--ts", "--tailwind", "--src-dir", "--no-git", "--yes"]
    },
    "assertions": {
      "shouldDetectFramework": "Next.js"
    }
  },
  "nuxtjs": {
    "name": "Nuxt.js Habit Tracker",
    "type": "clone",
    "repo": "https://github.com/zackha/habit",
    "path": ".",
    "branch": "master",
    "assertions": {
      "shouldDetectFramework": "Nuxt.js",
      "shouldHaveEndpoints": true,
      "minEndpoints": 2
    }
  }
}
